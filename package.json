{
  "name": "cdp-uploader",
  "version": "0.0.0",
  "description": "CDP Frontend Template",
  "main": ".server/index.js",
  "type": "module",
  "engines": {
    "node": ">=22.13.1"
  },
  "scripts": {
    "build": "run-s build:frontend build:server",
    "build:frontend": "NODE_ENV=production webpack",
    "build:server": "NODE_ENV=production babel --delete-dir-on-start --ignore \"**/*.test.js\" --ignore \"**/*.e2e.js\" --ignore \"**/__fixtures__\" --ignore \"**/test-helpers\" --copy-files --no-copy-ignored --out-dir ./.server ./src",
    "docker:dev": "NODE_ENV=development run-p frontend:watch server:watch",
    "predev": "npm run build:frontend",
    "dev": "run-p frontend:watch server:watch",
    "dev:debug": "run-p frontend:watch server:debug",
    "format": "prettier --write \"src/**/*.js\" \"**/*.{js,cjs,md,json,config.js,test.js}\"",
    "format:check": "prettier --check \"src/**/*.js\" \"**/*.{js,cjs,json,config.js,test.js}\"",
    "frontend:watch": "webpack --watch",
    "postinstall": "npm run setup:husky",
    "lint": "run-s lint:js lint:scss lint:types",
    "lint:fix": "eslint --cache --fix .",
    "lint:js": "eslint --cache .",
    "lint:scss": "stylelint \"src/**/*.scss\" --cache --cache-location .cache/stylelint --cache-strategy content --color --ignore-path .gitignore",
    "lint:types": "tsc --build tsconfig.json",
    "postversion": "git add package.json package-lock.json && git commit -m $npm_package_version",
    "test": "TZ=UTC jest --coverage --verbose",
    "test:e2e": "TZ=UTC jest --verbose --testMatch \"**/src/**/uploader.e2e.js\"",
    "test:watch": "jest --runTestsByPath ./test",
    "server:watch": "nodemon --exec tsx --enable-source-maps ./src",
    "server:debug": "nodemon --exec tsx --enable-source-maps --inspect=9239 ./src",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node --use-strict .",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@aws-sdk/client-s3": "3.556.0",
    "@aws-sdk/client-sqs": "3.556.0",
    "@aws-sdk/credential-providers": "3.556.0",
    "@aws-sdk/lib-storage": "3.556.0",
    "@babel/runtime": "7.24.7",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/boom": "10.0.1",
    "@hapi/hapi": "21.3.10",
    "@hapi/inert": "7.1.0",
    "@hapi/vision": "7.0.3",
    "aws-embedded-metrics": "4.1.1",
    "babel-plugin-module-resolver": "5.0.2",
    "convict": "6.2.4",
    "cssnano": "7.0.4",
    "cssnano-preset-default": "7.0.4",
    "date-fns": "3.6.0",
    "file-type": "^16.5.4",
    "filesize": "10.1.1",
    "form-data": "4.0.0",
    "govuk-frontend": "5.3.1",
    "hapi-pino": "12.1.0",
    "hapi-pulse": "3.0.1",
    "https-proxy-agent": "7.0.4",
    "ioredis": "5.4.1",
    "joi": "17.13.0",
    "lodash": "4.17.21",
    "mime-types": "2.1.35",
    "node-fetch": "^2.7.0",
    "nunjucks": "3.2.4",
    "pino": "9.2.0",
    "pino-pretty": "11.2.1",
    "qs": "6.12.1",
    "sqs-consumer": "9.1.0",
    "undici": "6.19.2",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.24.7",
    "@babel/core": "7.24.7",
    "@babel/preset-env": "7.24.7",
    "@types/convict": "6.1.6",
    "@types/hapi__catbox-memory": "4.1.8",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.17.7",
    "@types/node": "20.14.12",
    "@types/node-fetch": "2.6.11",
    "@types/nunjucks": "3.2.6",
    "@types/webpack-assets-manifest": "5.1.4",
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/parser": "7.17.0",
    "autoprefixer": "10.4.19",
    "babel-jest": "29.7.0",
    "babel-loader": "9.1.3",
    "babel-plugin-transform-import-meta": "2.2.1",
    "cheerio": "1.0.0-rc.12",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.2.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsdoc": "48.8.3",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "6.1.1",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "mini-css-extract-plugin": "2.9.0",
    "nodemon": "3.1.4",
    "npm-run-all": "4.1.5",
    "postcss-load-config": "^6.0.1",
    "postcss-loader": "^8.1.1",
    "prettier": "3.3.3",
    "resolve-url-loader": "5.0.0",
    "sass": "1.75.0",
    "sass-loader": "16.0.1",
    "source-map-loader": "5.0.0",
    "style-loader": "4.0.0",
    "stylelint": "16.4.0",
    "stylelint-config-gds": "2.0.0",
    "terser-webpack-plugin": "5.3.10",
    "tsx": "4.16.2",
    "typescript": "5.5.4",
    "webpack": "5.94.0",
    "webpack-assets-manifest": "5.2.1",
    "webpack-cli": "5.1.4"
  }
}
